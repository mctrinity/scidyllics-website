
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  name      String
  email     String
  company   String?
  message   String?
  createdAt DateTime @default(now())
}

model CaseStudy {
  id        String   @id @default(cuid())
  title     String
  summary   String
  impact    String
  createdAt DateTime @default(now())
}
